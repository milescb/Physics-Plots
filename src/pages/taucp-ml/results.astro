---
import PlotGrid from '../../layouts/PlotGrid.astro';
import taucpmlConfig from '../../data/taucp-ml.json';

const project = taucpmlConfig;
const subpage = project.subpages.find(s => s.id === 'results');

if (!project || !subpage) throw new Error('Page not found');
---

<PlotGrid
  projectName={project.name}
  subpageName={subpage.name}
  plots={subpage.plots}
  backLink="/taucp-ml"
/>
